(alias
 ((name runtest)
  (deps (float_test.exe))
  (action (bash "./float_test.exe"))))

(alias
 ((name runtest)
  (deps (pool_caml_modify_check.exe))
  (action (bash "./pool_caml_modify_check.exe"))))

(alias
 ((name runtest)
  (deps (just_raise.exe just_raise.expected-output uncaught_exception_test.sh))
  (action (bash "./uncaught_exception_test.sh"))))

(executables
 ((names (float_test pool_caml_modify_check just_raise))
  (flags (:standard -safe-string))
  (libraries (replace_caml_modify_for_testing
              core))
  (preprocess (pps (ppx_jane ppxlib.runner)))))



(jbuild_version 1)
